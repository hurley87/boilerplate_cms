		<!-- Site.Core.Helpers.Mailer settings -->
		<add key="smartsite.emailqueue" value="Network Library=DBMSSOCN;DATABASE=ss_email;UID=smartsite;PWD=ikrz;Address=emailDB;network library=dbmssocn111" />
